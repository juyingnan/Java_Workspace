<!-- xpath BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Sat Dec 22 14:57:16 CST 2012 -->
<bpel:process name="xpath" targetNamespace="ss.pku.edu.cn"
	suppressJoinFailure="yes" xmlns:tns="ss.pku.edu.cn"
	xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
	xmlns:ns1="http://www.w3.org/2001/XMLSchema">

	<!-- Import the client WSDL -->
	<bpel:import location="xpathArtifacts.wsdl" namespace="ss.pku.edu.cn"
		importType="http://schemas.xmlsoap.org/wsdl/" />

	<!-- ================================================================= -->
	<!-- PARTNERLINKS -->
	<!-- List of services participating in this BPEL process -->
	<!-- ================================================================= -->
	<bpel:partnerLinks>
		<!-- The 'client' role represents the requester of this service. -->
		<bpel:partnerLink name="client" partnerLinkType="tns:xpath"
			myRole="xpathProvider" />
	</bpel:partnerLinks>

	<!-- ================================================================= -->
	<!-- VARIABLES -->
	<!-- List of messages and XML documents used within this BPEL process -->
	<!-- ================================================================= -->
	<bpel:variables>
		<!-- Reference to the message passed as input during initiation -->
		<bpel:variable name="input" messageType="tns:xpathRequestMessage" />

		<!-- Reference to the message that will be returned to the requester -->
		<bpel:variable name="output" messageType="tns:xpathResponseMessage" />
        
        
        
        <bpel:variable name="vd" type="ns1:duration"></bpel:variable>
    </bpel:variables>

	<!-- ================================================================= -->
	<!-- ORCHESTRATION LOGIC -->
	<!-- Set of activities coordinating the flow of messages across the -->
	<!-- services integrated within this business process -->
	<!-- ================================================================= -->
	<bpel:sequence name="main">

		<!-- Receive input from requester. Note: This maps to operation defined 
			in xpath.wsdl -->
		<bpel:receive name="receiveInput" partnerLink="client"
			portType="tns:xpath" operation="process" variable="input"
			createInstance="yes" />
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="Assign">
            
            
            <bpel:copy>
                <bpel:from><bpel:literal><tns:xpathResponse xmlns:tns="ss.pku.edu.cn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>0</tns:result>
</tns:xpathResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    <![CDATA[$input.payload/tns:input * $input.payload/tns:input]]>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        
        <bpel:assign validate="no" name="Assign1">
            <bpel:copy>
                <bpel:from>
                    
                    
                    
                    <![CDATA[concat("PT",$input.payload/tns:input,"S")]]>
                </bpel:from>
                <bpel:to variable="vd"></bpel:to>
            </bpel:copy>
        </bpel:assign>
        <!--  
        <bpel:wait name="Wait">
            <bpel:for><![CDATA[$vd]]></bpel:for>
        </bpel:wait>
        -->
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:xpath"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

