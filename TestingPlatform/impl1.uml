            <ownedComment xmi:id="_uqlK4j7CEeGGRLl_T9D32g">
              <body>/** 
 * Sets the crossover rate calculator.
 * @param a_crossoverRateCalculator the new calculator
 * @author Chris Knowles
 * @since 2.0
 */
</body>
            </ownedComment>
            <ownedParameter xmi:id="_uqlK4T7CEeGGRLl_T9D32g" name="a_crossoverRateCalculator" type="_upY4ED7CEeGGRLl_T9D32g"/>
          </ownedOperation>
          <ownedOperation xmi:id="_uqlK4z7CEeGGRLl_T9D32g" name="compareTo" visibility="public">
            <ownedComment xmi:id="_uquU0D7CEeGGRLl_T9D32g">
              <body>/** 
 * Compares the given object to this one.
 * @param a_other the instance against which to compare this instance
