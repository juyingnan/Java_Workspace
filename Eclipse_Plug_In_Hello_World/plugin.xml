<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="Eclipse_Plug_In_Hello_World.actionSet">
         <menu
               label="My Plug-ins"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="Weather Report"
               icon="icons/sample.gif"
               class="eclipse_plug_in_hello_world.actions.SampleAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="eclipse_plug_in_hello_world.actions.SampleAction">
         </action>
      </actionSet>
   </extension>

</plugin>
