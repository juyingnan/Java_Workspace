<!-- OnlineShop BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Tue Nov 20 13:34:02 CST 2012 -->
<bpel:process name="OnlineShop" targetNamespace="ss.pku.edu.cn"
	suppressJoinFailure="yes" xmlns:tns="ss.pku.edu.cn"
	xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
	xmlns:ns1="http://cn.edu.pku.ss" xmlns:ns2="http://www.w3.org/2001/XMLSchema"
	xmlns:ns3="http://cn.edu.pku.ss.bank" xmlns:ns4="http://cn.edu.pku.ss.order" xmlns:ns5="http://cn.edu.pku.ss.stock">

	<!-- Import the client WSDL -->
    <bpel:import namespace="http://cn.edu.pku.ss.stock" location="StockService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import namespace="http://cn.edu.pku.ss.order"
		location="OrderIDGenerator.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
	<bpel:import namespace="http://cn.edu.pku.ss.bank"
		location="BankService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
	<bpel:import location="OnlineShopArtifacts.wsdl" namespace="ss.pku.edu.cn"
		importType="http://schemas.xmlsoap.org/wsdl/" />

	<!-- ================================================================= -->
	<!-- PARTNERLINKS -->
	<!-- List of services participating in this BPEL process -->
	<!-- ================================================================= -->
	<bpel:partnerLinks>
		<!-- The 'client' role represents the requester of this service. -->
		<bpel:partnerLink name="client" partnerLinkType="tns:OnlineShop"
			myRole="OnlineShopProvider" />



		<bpel:partnerLink name="bankPL" partnerLinkType="tns:bankPLT" partnerRole="bankProvider"></bpel:partnerLink>
		<bpel:partnerLink name="IDgeneratorPL"
			partnerLinkType="tns:IDgeneratorPLT" partnerRole="IDgeneratorProvider"></bpel:partnerLink>
        
        <bpel:partnerLink name="stockPL" partnerLinkType="tns:StockPLT" partnerRole="StockProvider"></bpel:partnerLink>
    </bpel:partnerLinks>

	<!-- ================================================================= -->
	<!-- VARIABLES -->
	<!-- List of messages and XML documents used within this BPEL process -->
	<!-- ================================================================= -->
	<bpel:variables>
		<!-- Reference to the message passed as input during initiation -->
		<bpel:variable name="input" messageType="tns:OnlineShopRequestMessage" />

		<!-- Reference to the message that will be returned to the requester -->
		<bpel:variable name="output" messageType="tns:OnlineShopResponseMessage" />
		<bpel:variable name="BankRequest" messageType="ns3:creditCardAuthorizationRequest"></bpel:variable>
		<bpel:variable name="BankResponse"
			messageType="ns3:creditCardAuthorizationResponse"></bpel:variable>
		<bpel:variable name="bankResult" type="ns2:boolean"></bpel:variable>

		<bpel:variable name="OrderID" type="ns2:string"></bpel:variable>
		<bpel:variable name="ToasterType" type="ns2:string"></bpel:variable>
		<bpel:variable name="IDgeneratorRequest" messageType="ns4:orderIDGeneratorRequest"></bpel:variable>
		<bpel:variable name="IDgeneratorResponse" messageType="ns4:orderIDGeneratorResponse"></bpel:variable>

        <bpel:variable name="StockRequest" messageType="ns5:stockInquiryRequest"></bpel:variable>
        <bpel:variable name="StockResponse" messageType="ns5:stockInquiryResponse"></bpel:variable>
        <bpel:variable name="StockResult" type="ns2:boolean"></bpel:variable>
        <bpel:variable name="finalResult" type="ns2:int"></bpel:variable>
        <bpel:variable name="isCanceled" type="ns2:boolean"></bpel:variable>
    </bpel:variables>

	<!-- ================================================================= -->
	<!-- ORCHESTRATION LOGIC -->
	<!-- Set of activities coordinating the flow of messages across the -->
	<!-- services integrated within this business process -->
	<!-- ================================================================= -->
    <bpel:sequence name="main"><!-- Receive input from requester. Note: 
			This maps to operation defined in OnlineShop.wsdl -->
		<bpel:receive name="ReceiveOrder" partnerLink="client"
			portType="tns:OnlineShop" operation="process" variable="input"
			createInstance="yes" />

		<!-- Generate reply to synchronous request -->

		<bpel:assign validate="no" name="orderToBankRequest">





			<bpel:copy>
				<bpel:from>
					<bpel:literal>
						<impl:creditCardAuthorization
							xmlns:impl="http://cn.edu.pku.ss.bank" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
							<impl:creditCardNo>impl:creditCardNo</impl:creditCardNo>
						</impl:creditCardAuthorization>
					</bpel:literal>
				</bpel:from>
				<bpel:to variable="BankRequest" part="parameters"></bpel:to>
			</bpel:copy>
			<bpel:copy>
				<bpel:from part="payload" variable="input">
					<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:CreditCardNo]]></bpel:query>
				</bpel:from>
				<bpel:to part="parameters" variable="BankRequest">
					<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns3:creditCardNo]]></bpel:query>
				</bpel:to>
			</bpel:copy>
		</bpel:assign>
		<bpel:invoke name="InvokeBankService" partnerLink="bankPL" operation="creditCardAuthorization" portType="ns3:BankService" inputVariable="BankRequest" outputVariable="BankResponse"></bpel:invoke>

		<bpel:assign validate="no" name="BankResponseToBankResult">

			<bpel:copy>
				<bpel:from part="parameters" variable="BankResponse">
					<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[ns3:creditCardAuthorizationReturn]]>
					</bpel:query>
				</bpel:from>
				<bpel:to variable="bankResult"></bpel:to>
			</bpel:copy>
		</bpel:assign>
		<bpel:if name="If_Bank_Response">
			<bpel:condition><![CDATA[$bankResult='true']]></bpel:condition>
			<bpel:sequence name="FurtherSequence">

				<bpel:assign validate="no" name="costumerID_Assign">


					<bpel:copy>
						<bpel:from>
							<bpel:literal>
								<impl:orderIDGenerator xmlns:impl="http://cn.edu.pku.ss.order"
									xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
									<impl:customerID>impl:customerID</impl:customerID>
								</impl:orderIDGenerator>
							</bpel:literal>
						</bpel:from>
						<bpel:to variable="IDgeneratorRequest" part="parameters"></bpel:to>
					</bpel:copy>
					<bpel:copy>
						<bpel:from part="payload" variable="input">
							<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                <![CDATA[tns:CustomerID]]>
							</bpel:query>
						</bpel:from>
						<bpel:to part="parameters" variable="IDgeneratorRequest">
							<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                <![CDATA[ns4:customerID]]>
							</bpel:query>
						</bpel:to>
					</bpel:copy>
				</bpel:assign>
				<bpel:invoke name="IDgeneratorInvoke" partnerLink="IDgeneratorPL"
					operation="orderIDGenerator" portType="ns4:OrderIDGenerator"
					inputVariable="IDgeneratorRequest" outputVariable="IDgeneratorResponse"></bpel:invoke>
				<bpel:assign validate="no" name="orderIDAssign">

					<bpel:copy>
						<bpel:from part="parameters" variable="IDgeneratorResponse">
							<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                <![CDATA[ns4:orderIDGeneratorReturn]]>
							</bpel:query>
						</bpel:from>
						<bpel:to variable="OrderID"></bpel:to>
					</bpel:copy>
				</bpel:assign>
                <bpel:assign validate="no" name="StockRequestAssign">
                    <bpel:copy>
                        <bpel:from><bpel:literal><impl:stockInquiry xmlns:impl="http://cn.edu.pku.ss.stock" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <impl:tosterType>impl:tosterType</impl:tosterType>
  <impl:quantity>0</impl:quantity>
</impl:stockInquiry>
</bpel:literal></bpel:from>
                        <bpel:to variable="StockRequest" part="parameters"></bpel:to>
                    </bpel:copy>
                    <bpel:copy>
                        <bpel:from part="payload" variable="input">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:ToasterType]]></bpel:query>
                        </bpel:from>
                        <bpel:to part="parameters" variable="StockRequest">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns5:tosterType]]></bpel:query>
                        </bpel:to>
                    </bpel:copy>
                    <bpel:copy>
                        <bpel:from part="payload" variable="input">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:Quantity]]></bpel:query>
                        </bpel:from>
                        <bpel:to part="parameters" variable="StockRequest">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns5:quantity]]></bpel:query>
                        </bpel:to>
                    </bpel:copy>
                </bpel:assign>
                <bpel:invoke name="StockInquiryInvoke" partnerLink="stockPL" operation="stockInquiry" portType="ns5:StockService" inputVariable="StockRequest" outputVariable="StockResponse"></bpel:invoke>
                <bpel:assign validate="no" name="StockResultAssign">
                    <bpel:copy>
                        <bpel:from part="parameters" variable="StockResponse">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns5:stockInquiryReturn]]></bpel:query>
                        </bpel:from>
                        <bpel:to variable="StockResult"></bpel:to>
                    </bpel:copy>
                </bpel:assign>
                <bpel:if name="If_Stock_Response">
                    <bpel:condition><![CDATA[$StockResult='true']]></bpel:condition>
                    <bpel:assign validate="no" name="OrderResult">
					
				
                        <bpel:copy>
                            <bpel:from>
                                <bpel:literal xml:space="preserve">1</bpel:literal>
                            </bpel:from>
                            <bpel:to variable="finalResult"></bpel:to>
                        </bpel:copy>
                    </bpel:assign>
                    <bpel:else>
                        <bpel:sequence>
                            <bpel:assign validate="no" name="isCaneledAssign">
                                <bpel:copy>
                                    <bpel:from part="payload" variable="input">
                                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:IsCanceled]]></bpel:query>
                                    </bpel:from>
                                    <bpel:to variable="isCanceled"></bpel:to>
                                </bpel:copy>
                            </bpel:assign>
                            <bpel:if name="If">
                                <bpel:condition><![CDATA[$isCanceled='true']]></bpel:condition>
                                <bpel:assign validate="no" name="OrderResult3">
                                    <bpel:copy><bpel:from>
                                    <bpel:literal xml:space="preserve">-1</bpel:literal>
                                </bpel:from>
                                <bpel:to variable="finalResult"></bpel:to>
                            
                                    </bpel:copy>
                            
                                </bpel:assign>
                                <bpel:else>
                                    <bpel:assign validate="no" name="OrderResult4">
                                        <bpel:copy>
                                            <bpel:from>
                                                <bpel:literal xml:space="preserve">2</bpel:literal>
                                            </bpel:from>
                                            <bpel:to variable="finalResult"></bpel:to>
                                        </bpel:copy>
                                    </bpel:assign>
                                </bpel:else>
                            </bpel:if>
                            
                        </bpel:sequence>
                    </bpel:else>
                </bpel:if>
                




			</bpel:sequence>
			<bpel:else>
				<bpel:sequence name="Rejection">
					<bpel:assign validate="no" name="OrderResult2">




						
						
                        <bpel:copy>
                            <bpel:from>
                                <bpel:literal xml:space="preserve">0</bpel:literal>
                            </bpel:from>
                            <bpel:to variable="finalResult"></bpel:to>
                        </bpel:copy>
                    </bpel:assign>

				</bpel:sequence>
			</bpel:else>
		</bpel:if>
        <bpel:assign validate="no" name="OrderResultToFinalResult">
            <bpel:copy>
                <bpel:from><bpel:literal><tns:OnlineShopResponse xmlns:tns="ss.pku.edu.cn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:PurchaseResult>tns:PurchaseResult</tns:PurchaseResult>
</tns:OnlineShopResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from variable="finalResult"></bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:PurchaseResult]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="FinalResultFeedback" partnerLink="client"
			operation="process" portType="tns:OnlineShop" variable="output" />

	</bpel:sequence>
</bpel:process>

